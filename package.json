{
  "name": "@djthoms/metric-parser",
  "version": "0.0.12",
  "description": "AST-based advanced mathematical parser written by Typescript.",
  "keywords": [
    "formula",
    "metric",
    "parser",
    "ast",
    "typescript",
    "validator"
  ],
  "homepage": "https://github.com/KennethanCeyer/metric-parser#readme",
  "bugs": {
    "url": "https://github.com/KennethanCeyer/metric-parser/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KennethanCeyer/metric-parser.git"
  },
  "license": "MIT",
  "author": "PIGNOSE",
  "main": "./dist/cjs/metric.parser.js",
  "module": "./dist/esm/metric.parser.js",
  "types": "./dist/types/metric.parser.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/metric.parser.d.ts",
      "require": "./dist/cjs/metric.parser.js",
      "import": "./dist/esm/metric.parser.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "is-ci || husky install",
    "prebuild": "rm -rf dist",
    "build": "npm-run-all -p build:*",
    "build:cjs": "esbuild --sourcemap --outdir=dist/cjs --format=cjs --target=node18 `./scripts/fd.sh`",
    "build:esm": "esbuild --sourcemap --outdir=dist/esm --format=esm --target=node18 `./scripts/fd.sh`",
    "build:types": "tsc -p tsconfig.build.json",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "test": "vitest --run",
    "typedoc": "typedoc --tsconfig tsconfig.build.json"
  },
  "devDependencies": {
    "@djthoms/eslint-config": "^6.0.1",
    "@djthoms/prettier-config": "^4.3.0",
    "@types/node": "^20.5.3",
    "@vitest/coverage-v8": "^0.34.2",
    "esbuild": "^0.19.2",
    "fd-find": "^1.2.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "lint-staged": "^14.0.1",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.1",
    "typedoc": "^0.24.8",
    "typescript": "^5.1.6",
    "vitest": "^0.34.2"
  }
}
